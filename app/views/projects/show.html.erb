<!-- Page Content -->
<div id="page-wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Todos os projetos</h1>
                <p id="notice"><%= notice %></p>
                <p>
                  <strong>Title:</strong>
                  <%= @project.title %>
                </p>
                
                <p>
                  <strong>Text:</strong>
                  <%= @project.text %>
                </p>
                
                <p>
                  <strong>Delivery date:</strong>
                  <%= @project.delivery_date %>
                </p>
                
                <p>
                  <strong>Creation date:</strong>
                  <%= @project.creation_date %>
                </p>
                
                <p>
                  <strong>Category:</strong>
                  <%= @project.category %>
                </p>
                
                <%= link_to 'Edit', edit_project_path(@project) %> |
                <%= link_to 'Back', projects_path %>
                
              
            </div>
            <!-- /.col-lg-12 -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</div>
<!-- /#page-wrapper -->
